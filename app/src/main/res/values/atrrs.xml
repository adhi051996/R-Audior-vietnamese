<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="TedBottomPickerImageView">
        <attr name="foreground" format="reference"/>
        <attr name="fit_mode" format="string"/>
    </declare-styleable>



    <declare-styleable name="TedBottomPickerSquareView">
        <attr name="matchHeightToWidth" format="boolean"/>
        <attr name="matchWidthToHeight" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="AspectRatioView">
        <attr name="view_aspectRatio" format="float"/>
    </declare-styleable>



</resources>